<div class="card">
  <%= link_to image_tag(prototype.image), prototype_path(prototype.id), method: :get, class: 'prototype-image'  %>
  <div class="card__body">
    <%= link_to prototype.title, prototype_path(prototype.id), method: :get %>
    <p class="card__summary">
      <%= prototype.catch_copy %>
    </p>
    <%= link_to prototype.user.name, user_path(prototype.user), method: :GET %>
  </div>
</div>